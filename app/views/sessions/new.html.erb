<h1>Admin Log In Here</h1>

<%= form_tag sessions_url do || %>
	<p>
		<%= label_tag :login %>
		<%= text_field_tag :login %>
	</p>
	
	<!-- <p>
			<%#= label_tag :email %>
			<%#= text_field_tag :email %>
		</p> -->
	
	<p>
		<%= label_tag :password %>
		<%= password_field_tag :password %>
	</p>
	
	<%= submit_tag "Log Me In, Homie!" %>
	
	<% end %>